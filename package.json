{
  "name": "backend-products",
  "version": "1.0.0",
  "description": "Product Management API with JWT authentication and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:init": "node dist/scripts/initDb.js",
    "db:migrate:neon": "ts-node src/scripts/migrateNeon.ts",
    "vercel-build": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript",
    "postgresql",
    "jwt",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "@types/pg": "^8.15.6",
    "@vercel/node": "^5.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
